{
	"manifest_version": 2,
	"name": "Open with Opener",
	"description": "Open nHentai code with Opener Studio",
	"version": "1.0",
	"permissions": [
		"activeTab",
		"https://*.instagram.com/*",
		"https://instagram.com/*",
		"https://*.twitter.com/*",
		"https://twitter.com/*",
		"https://*.facebook.com/*",
		"https://facebook.com/*",
		"https://*.reddit.com/*",
		"https://reddit.com/*",
		"https://*.4chan.org/*",
		"https://4chan.org/*",
		"https://*.4channel.org/*",
		"https://boards.4channel.org/*",
		"https://*.discordapp.com/*",
		"https://discordapp.com/*",
		"contextMenus",
		"tabs"
	],
	"background": {
		"scripts": ["index.js"]
	},
	"web_accessible_resources": ["index.js"],
	"icons": {
		"16": "opener.png",
		"48": "opener.png",
		"128": "opener.png"
	}
}
